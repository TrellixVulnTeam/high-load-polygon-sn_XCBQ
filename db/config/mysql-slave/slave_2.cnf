[mysqld]
server-id=3
log-bin=mysql-bin

binlog_format=ROW
gtid_mode=ON
enforce_gtid_consistency=ON
plugin-load=rpl_semi_sync_slave=semisync_slave.so
rpl_semi_sync_slave_enabled=1

log-slave-updates=1
auto_increment_increment=2
auto_increment_offset=2
datadir=/var/lib/mysql
read-only=1
slave-skip-errors = 1062
skip-host-cache
skip-name-resolve